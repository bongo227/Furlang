proc sort :: i32[5] items -> i32[5] { 
    n := 5
    for i := 0; i < n-1; i++ {
        for j := 0; j < n-i-1; j++ {
            if items[j] > items[j+1] {
                i32 temp = arr[j]
                arr[j] = arr[j+1]
                arr[j+1] = temp
            }
        }
    }
}

proc main :: -> i32 {
    sorted := sort(i32[5]{123, 32, 756, 23, 1000})
    return sorted[2]
}